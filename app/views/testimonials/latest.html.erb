<p id="notice"><%= notice %></p>

<h1>Listing Testimonials</h1>

<table>
  <thead>
    <tr>
      <th>Testimonial</th>
      <th>Title</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testimonials.each do |testimonial| %>
      <tr>
        <td><%= testimonial.testimonial %></td>
        <td><%= testimonial.title %></td>
        <td><%= testimonial.user_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
